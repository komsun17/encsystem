{
    "name": "encsystem/timetracking",
    "description": "Time tracking system for engineers",
    "type": "project",
    "require": {
        "phpoffice/phpspreadsheet": "^1.29"
    },
    "config": {
        "platform": {
            "php": "8.2"
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "phpoffice/phpspreadsheet": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "service/"
        }
    },
    "minimum-stability": "stable"
}